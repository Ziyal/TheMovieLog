@Html.Partial("Navbar-LoggedIn")

<div class="container">
    <h1>EXPLORE</h1>
    <hr>

    <h3>Users</h3>
    <table class="table table">
        <thead>
            <th>Name</th>
            <th>User Since</th>
        </thead>
        <tbody>
        @{
            foreach(var user in @ViewBag.Users) {
            <tr>
                <td><a href="/user_profile/@user.UserId">@user.FirstName @user.LastName</a></td>
                <td>@user.CreatedAt.ToString("MMMM yyyy")</td>

            </tr>
            }
        }
        </tbody>
    </table>
    <a href="explore"><button class="btn btn-default btn-block">Back</button></a>


</div>